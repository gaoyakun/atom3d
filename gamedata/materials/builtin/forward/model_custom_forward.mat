            </streamdefine>
            <rasterizer_state>
                <cullmode value="$cullmode" />
            </rasterizer_state>
            <vertexshader entry="vs_nodiffuse_notex_skinning_fs" target="vs_2_0" />
            <pixelshader entry="ps_notex_fs" target="ps_2_0" />
        </pass>
