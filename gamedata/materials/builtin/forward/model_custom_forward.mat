    <effect name="noAlphaBlend_noAlphaTest_noDiffuse_noTexture_skinning_Projectable_fs" scheme="custom" schemelayer="Solid">
        <requirements>
            <eq param="enableAlphaBlend" value="0" />
            <eq param="enableAlphaTest" value="0" />
