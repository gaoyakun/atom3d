            <eq param="hasVertexColor" value="0" />
            <eq param="hasDiffuseTexture" value="0" />
            <ne param="enableSkinning" value="0" />
			<eq param="projectable" value="0" />
        </requirements>
        <pass>
            <streamdefine>
                <stream semantec="position" />
